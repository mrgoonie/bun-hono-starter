{
  "name": "bun-hono-starter",
  "scripts": {
    "dev": "concurrently \"tailwindcss -i ./src/styles/global.css -o ./public/css/style.css --watch\" \"bun --watch src/server.ts\"",
    "start": "bun run src/server.ts",
    "db": "prisma generate",
    "db:push": "prisma db push",
    "db:all": "prisma generate && prisma db push"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.3.2",
    "@lemonsqueezy/lemonsqueezy.js": "^3.2.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/adapter-pg": "^5.19.0",
    "@prisma/client": "^5.19.0",
    "@trpc/server": "^10.45.2",
    "arctic": "^1.9.2",
    "diginext-utils": "^3.13.7",
    "dotenv": "^16.4.5",
    "hono": "^4.5.10",
    "lodash": "^4.17.21",
    "lucia": "^3.2.0",
    "oslo": "^1.2.1",
    "pg": "^8.12.0",
    "prisma": "^5.19.0",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}
